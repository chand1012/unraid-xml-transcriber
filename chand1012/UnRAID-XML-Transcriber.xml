<?xml version="1.0"?>
<Container version="2">
  <Name>UnRAID-XML-Transcriber</Name>
  <Repository>docker.pkg.github.com/chand1012/unraid-xml-transcriber/unraid-xml-transcriber:master</Repository>
  <Registry>https://github.com/chand1012/unraid-xml-transcriber</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview/>
  <Category/>
  <WebUI>http://[IP]:[PORT:5555]/servers</WebUI>
  <TemplateURL/>
  <Icon>https://i.imgur.com/ChA3BgJ.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1603398412</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>
    Transcribes output of unRAIDAPI to be used with Rainmeter.
  </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5555</HostPort>
        <ContainerPort>5555</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data/>
  <Environment>
    <Variable>
      <Value>[IP]</Value>
      <Name>API_HOST</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Container Port" Target="5555" Default="5555" Mode="tcp" Description="Container Port: 5555" Type="Port" Display="always" Required="true" Mask="false"></Config>
  <Config Name="API Host" Target="API_HOST" Default="[IP]" Mode="" Description="Container Variable: API_HOST" Type="Variable" Display="always" Required="true" Mask="false"></Config>
</Container>
